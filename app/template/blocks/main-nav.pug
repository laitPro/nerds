nav.main-nav.main-header__main-nav
  ul.nav-list
     - var nav = ['студия', 'клиенты', 'магазин', 'контакты']
      each item in nav
        li.nav-list__item
          - if (item == 'студия')
              a.nav-list__link.nav-list__link_active=item
          - else
              a(href="#").nav-list__link=item